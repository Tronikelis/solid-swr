{
    "name": "solid-swr",
    "description": "The beloved swr package ported over to solid",
    "version": "0.10.0",
    "type": "module",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "default": "./dist/index.cjs"
    },
    "keywords": [
        "solid",
        "solidjs",
        "swr",
        "solid-swr",
        "hooks",
        "request",
        "cache",
        "fetch"
    ],
    "files": [
        "dist"
    ],
    "author": "Tronikelis",
    "repository": {
        "type": "git",
        "url": "https://github.com/Tronikelis/solid-swr.git"
    },
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "tsup",
        "lint": "eslint . -c ./.eslintrc.cjs",
        "test": "jest",
        "tsc": "tsc --noEmit"
    },
    "peerDependencies": {
        "solid-js": "^1.7.8"
    },
    "dependencies": {
        "dequal": "^2.0.3"
    },
    "devDependencies": {
        "@babel/preset-env": "7.22.10",
        "@jest/globals": "29.6.2",
        "@solidjs/testing-library": "0.8.4",
        "babel-preset-solid": "1.7.7",
        "eslint": "8.47.0",
        "eslint-config-tronikelis-solid": "1.0.0",
        "jest": "29.6.2",
        "jest-environment-jsdom": "29.6.2",
        "prettier": "3.0.1",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "tsup": "7.2.0",
        "typescript": "5.0.2",
        "vite": "4.4.9",
        "vite-plugin-solid": "2.7.0",
        "vite-tsconfig-paths": "4.2.0",
        "whatwg-fetch": "3.6.17"
    }
}
