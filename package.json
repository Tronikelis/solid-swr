{
    "name": "solid-swr",
    "description": "The beloved swr package ported over to solid",
    "version": "5.0.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.js",
                "default": "./dist/index.d.ts"
            }
        },
        "./cache": {
            "import": {
                "types": "./dist/cache.js",
                "default": "./dist/cache.d.ts"
            }
        },
        "./extra": {
            "import": {
                "types": "./dist/extra.jsx",
                "default": "./dist/extra.d.ts"
            }
        }
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "solid",
        "solidjs",
        "swr",
        "solid-swr",
        "hooks",
        "request",
        "cache",
        "fetch"
    ],
    "author": "Tronikelis",
    "repository": {
        "type": "git",
        "url": "https://github.com/Tronikelis/solid-swr"
    },
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "tsc",
        "lint": "eslint . -c ./.eslintrc.cjs",
        "test": "vitest",
        "tsc": "tsc --noEmit"
    },
    "peerDependencies": {
        "solid-js": "1.x.x"
    },
    "dependencies": {},
    "devDependencies": {
        "@solidjs/testing-library": "0.8.5",
        "@testing-library/jest-dom": "6.3.0",
        "@types/node": "20.11.16",
        "eslint": "8.56.0",
        "eslint-config-tronikelis-solid": "10.0.0",
        "jsdom": "24.0.0",
        "prettier": "3.2.4",
        "prettier-config-tronikelis": "3.0.0",
        "typescript": "5.3.3",
        "vite": "5.0.12",
        "vite-plugin-solid": "2.9.1",
        "vite-tsconfig-paths": "4.3.1",
        "vitest": "1.2.2"
    },
    "browser": {},
    "typesVersions": {
        "*": {
            "cache": [
                "./dist/cache/index.d.ts"
            ],
            "extra": [
                "./dist/extra/index.d.ts"
            ]
        }
    },
    "packageManager": "pnpm@8.15.8+sha512.d1a029e1a447ad90bc96cd58b0fad486d2993d531856396f7babf2d83eb1823bb83c5a3d0fc18f675b2d10321d49eb161fece36fe8134aa5823ecd215feed392"
}
