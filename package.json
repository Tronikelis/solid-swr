{
    "name": "solidjs-swr",
    "description": "The beloved swr package ported over to solid",
    "version": "0.1.2",
    "type": "module",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "default": "./dist/index.cjs"
    },
    "files": [
        "dist"
    ],
    "author": "Tronikelis",
    "repository": {
        "type": "git",
        "url": "https://github.com/Tronikelis/solid-swr.git"
    },
    "scripts": {
        "dev": "vite",
        "preview": "vite preview",
        "build": "tsup",
        "lint": "eslint . -c ./.eslintrc.cjs",
        "prepublishOnly": "npm run build && npm run lint"
    },
    "peerDependencies": {
        "solid-js": "^1.7.8"
    },
    "dependencies": {
        "dequal": "^2.0.3"
    },
    "devDependencies": {
        "eslint": "8.47.0",
        "eslint-config-tronikelis-solid": "1.0.0",
        "prettier": "3.0.1",
        "tsup": "7.2.0",
        "typescript": "5.0.2",
        "vite": "4.4.5",
        "vite-plugin-solid": "2.7.0"
    }
}
